#include <stdio.h>

int main() {
    int num;

    // Get input from the user
    scanf("%d", &num);

    // Check if the number is positive, negative, or zero
    if (num > 0) {
        printf("The Given Number %d is Positive.\n", num);
    } else if (num < 0) {
        printf("The Given Number %d is Negative.\n", num);
    } else {
        printf("The Given Number %d is Zero.\n", num);
    }

    return 0;
}
